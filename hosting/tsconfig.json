{
	"compilerOptions": {
		"target": "es6",
		"module": "esNext",
		"moduleResolution": "node",
		"jsx": "react",
		"strict": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"baseUrl": "./",
		"paths": { "@/*": ["./src/*"], },
	},
	"include": [
		"./src",
	],
}
